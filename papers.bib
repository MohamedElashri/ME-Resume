@article{lhcbcollaborationSearch2007Mu2023,
  title = {Search for \${{D}}\^\{*\}(2007)\^0\textbackslash to\textbackslash mu\^+\textbackslash mu\^-\$ in \${{B}}\^-\textbackslash to\textbackslash pi\^-\textbackslash mu\^+\textbackslash mu\^-\$ Decays},
  author = {{LHCb collaboration} },
  date = {2023-04-05},
  eprint = {2304.01981},
  eprinttype = {arxiv},
  eprintclass = {hep-ex},
  doi = {10.48550/arXiv.2304.01981},
  url = {http://arxiv.org/abs/2304.01981},
  urldate = {2023-05-31},
  abstract = {The very rare \$D\^\{*\}(2007)\^0\textbackslash to\textbackslash mu\^+\textbackslash mu\^-\$ decay is searched for by analysing \$B\^-\textbackslash to\textbackslash pi\^-\textbackslash mu\^+\textbackslash mu\^-\$ decays. The analysis uses a sample of beauty mesons produced in proton-proton collisions collected with the LHCb detector between 2011 and 2018, corresponding to an integrated luminosity of 9 fb\$\^\{-1\}\$. The signal signature corresponds to simultaneous peaks in the \$\textbackslash mu\^+\textbackslash mu\^-\$ and \$\textbackslash pi\^-\textbackslash mu\^+\textbackslash mu\^-\$ invariant masses. No evidence for an excess of events over background is observed and an upper limit is set on the branching fraction of the decay at \$\{\textbackslash cal B\}(D\^\{*\}(2007)\^0\textbackslash to\textbackslash mu\^+\textbackslash mu\^-) {$<$} 2.6\textbackslash times 10\^\{-8\}\$ at \$90\textbackslash\%\$ confidence level. This is the first limit on the branching fraction of \$D\^\{*\}(2007)\^0\textbackslash to\textbackslash mu\^+\textbackslash mu\^-\$ decays and the most stringent limit on \$D\^\{*\}(2007)\^0\$ decays to leptonic final states. The analysis is the first search for a rare charm-meson decay exploiting production via beauty decays.},
  pubstate = {preprint},
  keywords = {High Energy Physics - Experiment},
  annotation = {0 citations (Inspire/arXiv) [2023-06-05]}
}
