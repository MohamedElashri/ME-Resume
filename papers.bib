@article{collaborationMeasurementMassDifference2023,
  title = {Measurement of the Mass Difference and Relative Production Rate of the \$\textbackslash{{Omega}}\^-\_b\$ and \$\textbackslash{{Xi}}\^-\_b\$ Baryons},
  author = {{LHCb collaboration}},
  date = {2023-05-24},
  url = {https://arxiv.org/abs/2305.15329v1},
  urldate = {2023-05-31},
  abstract = {The mass difference between the \$\textbackslash Omega\^-\_b\$ and \$\textbackslash Xi\^-\_b\$ baryons is measured using proton-proton collision data collected by the LHCb experiment, corresponding to an integrated luminosity of \$9 \textbackslash, \textbackslash text\{fb\}\^\{-1\}\$, and is found to be \textbackslash begin\{equation\} m(\textbackslash Omega\^-\_b)- m(\textbackslash Xi\^-\_b) = 248.54 \textbackslash pm 0.51 \textbackslash text\{(stat)\} \textbackslash pm 0.38 \textbackslash text\{(syst)\} \textbackslash, \textbackslash text\{MeV\}/c\^2. \textbackslash end\{equation\} The mass of the \$\textbackslash Omega\^-\_b\$ baryon is measured to be \textbackslash begin\{equation\} m(\textbackslash Omega\^-\_b)= 6045.9 \textbackslash pm 0.5 \textbackslash text\{(stat)\} \textbackslash pm 0.6 \textbackslash text\{(syst)\} \textbackslash, \textbackslash text\{MeV\}/c\^2. \textbackslash end\{equation\} This is the most precise determination of the \$\textbackslash Omega\^-\_b\$ mass to date. In addition, the production rate of \$\textbackslash Omega\^-\_b\$ baryons relative to that of \$\textbackslash Xi\^-\_b\$ baryons is measured for the first time in \$pp\$ collisions, using an LHCb dataset collected at a center-of-mass energy of \$13 \textbackslash, \textbackslash text\{TeV\}\$ and corresponding to an integrated luminosity of \$6\textbackslash,\textbackslash text\{fb\}\^\{-1\}\$. Reconstructing beauty baryons in the kinematic region \$2 {$<$} \textbackslash eta {$<$} 6\$ and \$p\_T {$<$} 20\textbackslash,\textbackslash text\{GeV\}/c\$ with their decays to a \$J/\textbackslash psi\$ meson and a hyperon, the ratio \textbackslash begin\{equation\} \textbackslash frac\{f\_\{\textbackslash Omega\^-\_b\}\}\{f\_\{\textbackslash Xi\^-\_b\}\}\textbackslash times\textbackslash frac\{\textbackslash mathcal\{B\}(\textbackslash Omega\^-\_b \textbackslash to J/\textbackslash psi \textbackslash Omega\^-)\}\{\textbackslash mathcal\{B\}(\textbackslash Xi\^-\_b \textbackslash to J/\textbackslash psi \textbackslash Xi\^-)\} = 0.120 \textbackslash pm 0.008 \textbackslash text\{(stat)\} \textbackslash pm 0.008 \textbackslash text\{(syst)\}, \textbackslash end\{equation\} is obtained, where \$f\_\{\textbackslash Omega\^-\_b\}\$ and \$f\_\{\textbackslash Xi\^-\_b\}\$ are the fragmentation fractions of \$b\$ quarks into \$\textbackslash Omega\^-\_b\$ and \$\textbackslash Xi\^-\_b\$ baryons, respectively, and \$\textbackslash mathcal\{B\}\$ represents the branching fractions of their respective decays.},
  langid = {english},
  organization = {{arXiv.org}},
  annotation = {1 citations (Inspire/arXiv) [2023-06-05]}
}

@article{collaborationPrecisionMeasurementItCP2023,
  title = {Precision Measurement of \$\textbackslash it\{\vphantom\}{{CP}}\vphantom\{\} \$ Violation in the Penguin-Mediated Decay \${{B}}\_s\^\{0\}\textbackslash rightarrow\textbackslash phi\textbackslash phi\$},
  author = {{LHCb collaboration}},
  date = {2023-04-13},
  url = {https://arxiv.org/abs/2304.06198v2},
  urldate = {2023-05-31},
  abstract = {A flavor-tagged time-dependent angular analysis of the decay \$B\_s\^\{0\}\textbackslash rightarrow\textbackslash phi\textbackslash phi\$ is performed using \$pp\$ collision data collected by the LHCb experiment at \$\textbackslash sqrt\{s\}=13\$ TeV, the center-of-mass energy of 13 TeV, corresponding to an integrated luminosity of 6 fb\^\{-1\}. The \$\textbackslash it\{CP\}\$-violating phase and direct \$\textbackslash it\{CP\}\$-violation parameter are measured to be \$\textbackslash phi\_\{s\textbackslash bar\{s\}s\} = -0.042 \textbackslash pm 0.075 \textbackslash pm 0.009 \$ rad and \$|\textbackslash lambda|=1.004\textbackslash pm 0.030 \textbackslash pm 0.009 \$, respectively, assuming the same values for all polarization states of the \$\textbackslash phi\textbackslash phi\$ system. In these results, the first uncertainties are statistical and the second systematic. These parameters are also determined separately for each polarization state, showing no evidence for polarization dependence. The results are combined with previous LHCb measurements using \$pp\$ collisions at center-of-mass energies of 7 and 8 TeV, yielding \$\textbackslash phi\_\{s\textbackslash bar\{s\}s\} = -0.074 \textbackslash pm 0.069 \$ rad and \$|\textbackslash lambda|=1.009 \textbackslash pm 0.030\$. This is the most precise study of time-dependent \$\textbackslash it\{CP\} \$ violation in a penguin-dominated \$B\$ meson decay. The results are consistent with \$\textbackslash it\{CP\} \$ symmetry and with the Standard Model predictions.},
  langid = {english},
  organization = {{arXiv.org}},
  annotation = {1 citations (Inspire/arXiv) [2023-06-05]}
}

@article{collaborationStudyCharmoniumDecays2023,
  title = {Study of Charmonium Decays to \${{K}}\^0\_{{S K}} \textbackslash pi\$ in the \${{B}} \textbackslash to ({{K}}\^0\_{{S K}} \textbackslash pi) {{K}}\$ Channels},
  author = {{LHCb collaboration}},
  date = {2023-04-28},
  url = {https://arxiv.org/abs/2304.14891v1},
  urldate = {2023-05-31},
  abstract = {A study of the \$B\^+\textbackslash to K\^0\_SK\^+K\^-\textbackslash pi\^+\$ and \$B\^+\textbackslash to K\^0\_SK\^+K\^+\textbackslash pi\^-\$ decays is performed using proton-proton collisions at center-of-mass energies of 7, 8 and 13 TeV at the LHCb experiment. The \$K\^0\_SK \textbackslash pi\$ invariant mass spectra from both decay modes reveal a rich content of charmonium resonances. New precise measurements of the \$\textbackslash eta\_c\$ and \$\textbackslash eta\_c(2S)\$ resonance parameters are performed and branching fraction measurements are obtained for \$B\^+\$ decays to \$\textbackslash eta\_c\$, \$J/\textbackslash psi\$, \$\textbackslash eta\_c(2S)\$ and \$\textbackslash chi\_\{c1\}\$ resonances. In particular, the first observation and branching fraction measurement of \$B\^+ \textbackslash to \textbackslash chi\_\{c0\} K\^0 \textbackslash pi\^+\$ is reported as well as first measurements of the \$B\^+\textbackslash to K\^0K\^+K\^-\textbackslash pi\^+\$ and \$B\^+\textbackslash to K\^0K\^+K\^+\textbackslash pi\^-\$ branching fractions. Dalitz plot analyses of \$\textbackslash eta\_c \textbackslash to K\^0\_SK\textbackslash pi\$ and \$\textbackslash eta\_c(2S) \textbackslash to K\^0\_SK\textbackslash pi\$ decays are performed. A new measurement of the amplitude and phase of the \$K \textbackslash pi\$ \$S\$-wave as functions of the \$K \textbackslash pi\$ mass is performed, together with measurements of the \$K\^*\_0(1430)\$, \$K\^*\_0(1950)\$ and \$a\_0(1700)\$ parameters. Finally, the branching fractions of \$\textbackslash chi\_\{c1\}\$ decays to \$K\^*\$ resonances are also measured.},
  langid = {english},
  organization = {{arXiv.org}},
  annotation = {1 citations (Inspire/arXiv) [2023-06-05]}
}

@article{collaborationTestLeptonFlavour2023,
  title = {Test of Lepton Flavour Universality Using \${{B}}\^0 \textbackslash to {{D}}\^\{*-\}\textbackslash tau\^+\textbackslash nu\_\{\textbackslash tau\}\$ Decays with Hadronic \$\textbackslash tau\$ Channels},
  author = {{LHCb collaboration}},
  date = {2023-05-02},
  url = {https://arxiv.org/abs/2305.01463v1},
  urldate = {2023-05-31},
  abstract = {The branching fraction \$\textbackslash mathcal\{B\}(B\^0 \textbackslash to D\^\{*-\}\textbackslash tau\^+\textbackslash nu\_\{\textbackslash tau\})\$ is measured relative to that of the normalisation mode \$B\^0 \textbackslash to D\^\{*-\}\textbackslash pi\^+\textbackslash pi\^-\textbackslash pi\^+\$ using hadronic \$\textbackslash tau\^+ \textbackslash to \textbackslash pi\^+\textbackslash pi\^-\textbackslash pi\^+(\textbackslash pi\^0)\textbackslash bar\{\textbackslash nu\}\_\{\textbackslash tau\}\$ decays in proton-proton collision data at a centre-of-mass energy of 13 TeV collected by the LHCb experiment, corresponding to an integrated luminosity of 2 fb\$\^\{-1\}\$. The measured ratio is \$\textbackslash mathcal\{B\}(B\^0 \textbackslash to D\^\{*-\}\textbackslash tau\^+\textbackslash nu\_\{\textbackslash tau\})/\textbackslash mathcal\{B\}(B\^0 \textbackslash to D\^\{*-\}\textbackslash pi\^+\textbackslash pi\^-\textbackslash pi\^+)= 1.70 \textbackslash pm 0.10\^\{+0.11\}\_\{-0.10\}\$, where the first uncertainty is statistical and the second is related to systematic effects. Using established branching fractions for the \$B\^0 \textbackslash to D\^\{*-\}\textbackslash pi\^+\textbackslash pi\^-\textbackslash pi\^+\$ and \$B\^0 \textbackslash to D\^\{*-\} \textbackslash mu\^+\textbackslash nu\_\textbackslash mu\$ modes, the lepton universality test, \$\textbackslash mathcal\{R\}(D\^\{*-\}) \textbackslash equiv \textbackslash mathcal\{B\}(B\^0 \textbackslash to D\^\{*-\}\textbackslash tau\^+\textbackslash nu\_\{\textbackslash tau\})/\textbackslash mathcal\{B\}(B\^0 \textbackslash to D\^\{*-\} \textbackslash mu\^+\textbackslash nu\_\textbackslash mu)\$ is calculated, \$\$ \textbackslash mathcal\{R\}(D\^\{*-\}) = 0.247 \textbackslash pm 0.015 \textbackslash pm 0.015 \textbackslash pm 0.012\textbackslash, , \$\$ where the third uncertainty is due to the uncertainties on the external branching fractions. This result is consistent with the Standard Model prediction and with previous measurements.},
  langid = {english},
  organization = {{arXiv.org}},
  annotation = {7 citations (Inspire/arXiv) [2023-06-05]}
}

