@thesis{elashriStripHitResolution2017,
  title = {Strip Hit Resolution of {{CMS Tracker}} Analysis},
  author = {Elashri, Mohamed},
  date = {2017-06-14},
  doi = {10.13140/RG.2.2.11136.84480},
  abstract = {The CMS silicon tracker consists of two tracking devices utilizing semiconductor technology: the inner pixel and the outer strip detectors. They operate in a high-occupancy and high-radiation environment presented by particle collisions in the LHC. The tracker detectors occupy the region around the center of CMS, where the LHC beams collide, between 4 cm and 110 cm in radius and up to 280 cm along the beam axis. The pixel detector consists of 66 million pixels, covering about 1 m\^2 total area. It is surrounded by the strip tracker with 10 million read-out channels covering about 200 m\^2 total area. In this study, details are given about the performance of the strip tracker at high occupancy with respect to local observables such as signal to noise ratio and hit reconstruction efficiency. Studies of strip hit resolution that affects the quality of track reconstruction.},
  file = {/Users/mohamedelashri/Zotero/storage/K5WDHL3W/Elashri_2017_Strip hit resolution of CMS Tracker analysis.pdf}
}

